{
  "parser": "babel-eslint",
  "extends": [
    "airbnb",
    "plugin:jest/recommended"
  ],
  "plugins": [
    "react",
    "jsx-a11y",
    "import",
    "jest"
  ],
  "env": {
    "jest/globals": true
  },
  "rules": {
    "strict": 0,
    "react/no-typos": 0
  },
  "globals": {
    "CustomEvent": true,
    "window": true,
    "document": true,
    "File": true
  }
}
